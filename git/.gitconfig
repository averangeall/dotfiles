[merge]
	tool = vimdiff
[core]
	editor = vim
[color]
	ui = true
[alias]
	graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short-local
[push]
	default = current
[log]
	date = iso-local
[pull]
	rebase = true
